docker inspect sab-data || docker run --name sab-data -v /sab --entrypoint true fish/sabnzbd
docker inspect sab      || docker run --name sab -d --volumes-from sab-data -v /s3:/s3 --net=host --entrypoint python fish/sabnzbd /sabnzb/SABnzbd.py -s $PIP:8080
